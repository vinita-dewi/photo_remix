    "version": "0.2.0",
    "configurations": [
        {
            "name": "photo_remix (Debug)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": ["--dart-define=STORAGE_EMULATOR_HOST=10.0.2.2"]
        },
        {
            "name": "photo_remix (Debug, software rendering)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define=STORAGE_EMULATOR_HOST=10.0.2.2",
                "--enable-software-rendering"
            ]
        },
        {
            "name": "photo_remix (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "photo_remix (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}
